{
  "bracketSameLine": false,
  "singleQuote": true,
  "printWidth": 120,
  "useTabs": false,
  "tabWidth": 2,
  "trailingComma": "es5",
  "arrowParens": "avoid",
  "semi": true,
  "plugins": ["prettier-plugin-antlers", "prettier-plugin-tailwindcss"],
  "overrides": [
    {
      "files": ["*.antlers.html"],
      "options": {
        "parser": "antlers"
      }
    }
  ]
}
